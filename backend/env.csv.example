# CSV Data Loading Configuration
# Copy this to .env and modify as needed

# Enable CSV data loading on application startup
LOAD_CSV_DATA=true

# Directory containing CSV files (relative to backend directory)
CSV_DATA_DIR=data/csv

# Force reload CSV data even if data already exists
FORCE_RELOAD_CSV=false

# Other application settings
DATABASE_URL=sqlite:///./data/church_course_tracker.db
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=120
ENVIRONMENT=development
DEBUG=true
